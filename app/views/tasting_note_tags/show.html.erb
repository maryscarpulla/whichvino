<div class="page-header">
  <h3>Tasting Note Tag #<%= @tasting_note_tag.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Tasting note name</dt>
      <dd>
        <% if @tasting_note_tag.tasting_note.present? %>
          <a href="/wines/<%= @tasting_note_tag.tasting_note_name %>">
            <%= @tasting_note_tag.tasting_note.wine_name %>
          </a>
        <% end %>
      </dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/tasting_note_tags" class="btn btn-primary">
        Back
      </a>
      <a href="/tasting_note_tags/<%= @tasting_note_tag.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_tasting_note_tag/<%= @tasting_note_tag.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
