<div class="page-header">
  <h3>Suggested Wine #<%= @suggested_wine.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Wine</dt>
      <dd>
        <% if @suggested_wine.wine.present? %>
          <a href="/wines/<%= @suggested_wine.wine_id %>">
            <%= @suggested_wine.wine.wine_name %>
          </a>
        <% end %>
      </dd>

      <dt>User id from</dt>
      <dd>
        <% if @suggested_wine.user_id_from.present? %>
          <a href="/users/<%= @suggested_wine.user_id_from %>">
            <%= @suggested_wine.user_id_from.created_at %>
          </a>
        <% end %>
      </dd>

      <dt>User id to</dt>
      <dd>
        <% if @suggested_wine.user_id_to.present? %>
          <a href="/users/<%= @suggested_wine.user_id_to %>">
            <%= @suggested_wine.user_id_to.created_at %>
          </a>
        <% end %>
      </dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/suggested_wines" class="btn btn-primary">
        Back
      </a>
      <a href="/suggested_wines/<%= @suggested_wine.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_suggested_wine/<%= @suggested_wine.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
